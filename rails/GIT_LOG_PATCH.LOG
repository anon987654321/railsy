+class InfiniteScrollReflex < ApplicationReflex
+  include Pagy::Backend
+
+  attr_reader :collection
+
+  def load_more
+    cable_ready.insert_adjacent_html(
+      selector: selector,
+      html: render(collection, layout: false),
+      position: position
+    ).broadcast
+  end
+
+  def page
+    element.dataset["next_page"].to_i
+  end
+
+  def position
+    "beforebegin"
+  end
+
+  def selector
+    "#sentinel"
+  end
+end
