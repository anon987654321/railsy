<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <title>403: <%= t :forbidden %></title>
    <style type="text/css">
      * {
        margin: 0;
        padding: 0;
      }
      body {
        font: 13px Arial, sans-serif;
        padding: 10px;
      }
      h1 {
        font-size: 30px;
        margin: 2px 0 0 -1px;
      }
      p {
        color: #888;
        margin-top: 50px;
      }
      a {
        color: #19558d;
        text-decoration: underline;
      }
    </style>
  </head>
  <body>
    <h1>403: <%= t :forbidden %></h1>
    <p><%= translate_with_link(:outdated_browser, google_chrome: "http://google.com/chrome/browser/") %></p>
    <%= render partial: "shared/google_analytics" %>
  </body>
</html>

